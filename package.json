{
    "name": "DevelopmentTools",
    "version": "1.0.0",
    "description": "Домашняя работа на курсе Инструменты разработки в SkyPro",
    "main": "index.js",
    "scripts": {
        "test2": "echo \"Error: no test specified\" && exit 1",
        "test": "jest --collect-coverage",
        "start2": "http-server -p 3003",
        "start": "cross-env NODE_ENV=development webpack-dev-server --open",
        "format": "prettier --write ./src",
        "lint:css": "stylelint ./src/**.css",
        "lint": "npm run lint:js && npm run lint:css",
        "lint:js": "eslint \"**/*.ts\"",
        "clean": "rm -rf node_modules/",
        "build": "cross-env NODE_ENV=production webpack",
        "prepare": "husky install"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "identity-obj-proxy"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Livanenkova/DevelopmentToolsSkyPro.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Livanenkova/DevelopmentToolsSkyPro/issues"
    },
    "homepage": "https://github.com/Livanenkova/DevelopmentToolsSkyPro#readme",
    "dependencies": {
        "@babel/preset-typescript": "^7.16.7",
        "@types/jest": "^27.4.1",
        "http-server": "^14.1.0",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@babel/core": "^7.17.8",
        "@babel/eslint-parser": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@types/lodash": "^4.14.181",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "babel-loader": "^8.2.4",
        "copy-webpack-plugin": "^10.2.4",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^3.4.1",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^7.0.0",
        "identity-obj-proxy": "^3.0.0",
        "image-webpack-loader": "^8.1.0",
        "jest": "^27.5.1",
        "mini-css-extract-plugin": "^2.6.0",
        "prettier": "^2.6.0",
        "style-loader": "^3.3.1",
        "stylelint": "^14.6.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-prettier": "^2.0.0",
